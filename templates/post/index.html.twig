{% extends('base.html.twig') %}

{% block body %}

    <h2>List of all posts</h2>
    <br>
    <table class="table table-dark table-striped">
        <thead>
        <tr>
            <td>ID</td>
            <td>Title</td>
        </tr>
        </thead>
        <tbody>
        {% for post in posts %}
            <tr>
                <td>{{ post.id }}</td>
                <td>{{ post.title }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
